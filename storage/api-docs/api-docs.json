    "paths": {
        "/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register",
                "description": "Register api.",
                "operationId": "/register",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password",
                                    "c_password"
                                ],
                                "properties": {
                                    "name": {
                                        "title": "required|string",
                                        "type": "string",
                                        "example": "Admin Zeus"
                                    },
                                    "email": {
                                        "title": "required|email|unique:users,email",
                                        "type": "string",
                                        "example": "admin@zeus.vision"
                                    },
                                    "password": {
                                        "title": "required|string",
                                        "type": "string",
                                        "example": ""
                                    },
                                    "c_password": {
                                        "title": "required|same:password",
                                        "type": "string",
                                        "example": ""
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password",
                                    "c_password"
                                ],
                                "properties": {
                                    "name": {
                                        "title": "required|string",
                                        "type": "string",
                                        "example": "Admin Zeus"
                                    },
                                    "email": {
                                        "title": "required|email|unique:users,email",
                                        "type": "string",
                                        "example": "admin@zeus.vision"
                                    },
                                    "password": {
                                        "title": "required|string",
                                        "type": "string",
                                        "example": ""
                                    },
                                    "c_password": {
                                        "title": "required|same:password",
                                        "type": "string",
                                        "example": ""
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "example": "Successful authentication."
                                        },
                                        "data": {
                                            "description": "these are the fields of the request",
                                            "properties": {
                                                "token_type": {
                                                    "example": "Bearer"
                                                },
                                                "expires_at": {
                                                    "example": "2022-03-02 17:10:15"
                                                },
                                                "access_token": {
                                                    "example": "11|qJdB9zfmvWUEnKhxNExfYogIA7CZvUx7MI9GvcHl"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Error: Unprocessable Entity",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "example": "The given data was invalid."
                                        },
                                        "errors": {
                                            "properties": {
                                                "name": {
                                                    "example": [
                                                        "The name field is required."
                                                    ]
                                                },
                                                "email": {
                                                    "example": [
                                                        "The email field is required."
                                                    ]
                                                },
                                                "password": {
                                                    "example": [
                                                        "The password field is required."
                                                    ]
                                                },
                                                "c_password": {
                                                    "example": [
                                                        "The c_password field is required."
                                                    ]
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "$ref": "#/components/requestBodies/response_500"
                    }
                }
            }
        },
